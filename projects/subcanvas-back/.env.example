# 서버 설정
PORT=3000
NODE_ENV=development

# 데이터베이스 설정 (Prisma 직접 연결용)
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/subcanvas?schema=public"

# JWT 설정
JWT_SECRET=subcanvas-secret-key
JWT_EXPIRES_IN=1d

# Supabase 설정
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key
SUPABASE_BUCKET_NAME=subcanvas-storage

# AWS S3 설정 (대체 스토리지 옵션)
AWS_ACCESS_KEY_ID=your_aws_key
AWS_SECRET_ACCESS_KEY=your_aws_secret
AWS_S3_BUCKET=subcanvas-storage
AWS_REGION=ap-northeast-2
